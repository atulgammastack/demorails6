<section class="users-section">
  <ul class="users-list">
    <% @users.each do |user| %>
      <li>
        Name: <%= user.username %>
        <span class="profile-link">
          <%= link_to 'See Profile',  user_path(user), class: 'profile-link' %> 
        </span>
        <%= render 'friendship_realation', object: user %>
      </li><br>
    <% end %>
  </ul>
</section>
